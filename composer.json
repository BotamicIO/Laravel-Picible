{
    "name": "faustbrian/laravel-picible",
    "description": "Picture attachment and management for your Eloquent models.",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-messageable",
        "Laravel Messageable",
        "Brian Faust",
        "faustbrian",
        "laravel-picible"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6 || ^7.0",
        "faustbrian/laravel-service-provider": "^1.0",
        "intervention/image": "2.*",
        "graham-campbell/flysystem": "^3.3"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Picible\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Picible\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
